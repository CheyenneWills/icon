unset ICONX IPATH LPATH FPATH

for i in `cat $1.lst`
do
   rm -f local/$i.out
   echo "Testing $i"
   ../../bin/icont -s $i.icn || continue
   if test -r $i.dat
   then
      ./$i <$i.dat >local/$i.out 2>&1
   else
      ./$i >local/$i.out 2>&1
   fi
   diff stand/$i.out local/$i.out
   rm -f $i
done
